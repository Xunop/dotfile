{
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct",
            "settings": {
                "domainStrategy": "UseIPv4"
            },
            "streamSettings": {
                "sockopt": {
                    "mark": 2
                }
            }
        },

        {
            "tag": "xun-proxy-jp",
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "xun_jp_domain", // Address, domain name or IP of the server
                        "port": 443, // Port, consistent with the server
                        "users": [
                            {
                                "id": "xun_jp_uuid",
                                "encryption": "none",
                                "flow": "xtls-rprx-vision"
                            }
                        ]
                    }
                ]
            },
            
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "", // SNI, if "address" is filled with the domain name of the server, it can be left blank. If "address" is filled with the IP of the server, fill in the domain name contained in the certificate of the server here
                    "allowInsecure": false,
                    "fingerprint": "firefox" // Used to configure the fingerprint of the specified TLS Client Hello, Xray will simulate the TLS fingerprint through the uTLS library, or generate it randomly
                }
            }
        },
        

        {
            "tag": "ming-proxy-jp",
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "ming_jp_domain", // Address, domain name or IP of the server
                        "port": 443, // Port, consistent with the server
                        "users": [
                            {
                                "id": "ming_jp_uuid",
                                "encryption": "none",
                                "flow": "xtls-rprx-vision"
                            }
                        ]
                    }
                ]
            },
            
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "", // SNI, if "address" is filled with the domain name of the server, it can be left blank. If "address" is filled with the IP of the server, fill in the domain name contained in the certificate of the server here
                    "allowInsecure": false,
                    "fingerprint": "firefox" // Used to configure the fingerprint of the specified TLS Client Hello, Xray will simulate the TLS fingerprint through the uTLS library, or generate it randomly
                }
            }
        },

        
        {
            "tag": "z-proxy-hk",
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "z_hk_domain", // Address, domain name or IP of the server
                        "port": 443, // Port, consistent with the server
                        "users": [
                            {
                                "id": "z_hk_uuid", // User ID, consistent with the server
                                "encryption": "none",
                                "flow": "xtls-rprx-vision"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "show": false, 
                    "serverName": "www.ujs.edu.cn", // SNI, if "address" is filled with the domain name of the server, it can be left blank. If "address" is filled with the IP of the server, fill in the domain name contained in the certificate of the server here
                    "publicKey": "z_hk_publicKey",
                    "shortId": "ffffffff",
                    "allowInsecure": false,
                    "fingerprint": "firefox", // Used to configure the fingerprint of the specified TLS Client Hello, Xray will simulate the TLS fingerprint through the uTLS library, or generate it randomly
                    "spiderX": "" // 爬虫初始路径与参数，建议每个客户端不同
                },
                "sockopt": {
                    "mark": 2
                }
            }
        },

        {
            "tag": "z-proxy-jp",
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "z_jp_domain", // Address, domain name or IP of the server
                        "port": 443, // Port, consistent with the server
                        "users": [
                            {
                                "id": "z_jp_uuid",
                                "encryption": "none",
                                "flow": "xtls-rprx-vision"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "show": false, 
                    "serverName": "www.ujs.edu.cn", // SNI, if "address" is filled with the domain name of the server, it can be left blank. If "address" is filled with the IP of the server, fill in the domain name contained in the certificate of the server here
                    "publicKey": "z_jp_publicKey",
                    "shortId": "00000000",
                    "allowInsecure": false,
                    "fingerprint": "firefox", // Used to configure the fingerprint of the specified TLS Client Hello, Xray will simulate the TLS fingerprint through the uTLS library, or generate it randomly
                    "spiderX": "" // 爬虫初始路径与参数，建议每个客户端不同
                },
                "sockopt": {
                    "mark": 2
                }
            }
        },
        
        {
            "tag": "block",
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            }
        },

        {
            "tag": "dns-out",
            "protocol": "dns",
            "settings": {
              "address": "8.8.8.8"
            },
            "proxySettings": {
              "tag": "ming-proxy-jp"
            },
            "streamSettings": {
                "sockopt": {
                    "mark": 2
                }
            }
        }
    ]
}
