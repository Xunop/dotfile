default partial alphanumeric_keys
xkb_symbols "basic" {
    include "us"
    include "custom(casp_esc)"
};

// caps lock as super, super as escape
partial modifier_keys
xkb_symbols "casp_esc" {
    key <CAPS> { [ Super_L, Super_L ] };
    // key <LWIN> { [ Escape, Escape ] };
    key <RTSH> { [ Escape, Escape ] };
};

